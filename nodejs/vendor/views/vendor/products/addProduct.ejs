        <div class="container-fluid">

            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="mx-sm-30 mx-20 ">
                            <!-- Start: card -->
                            <div class="card  add-product p-sm-30 p-20 my-30" style="background: #fff;">
                                <div class="card-header">
                                    <h6 class="fw-500 " style="text-transform: none;">Mahsulot qo'shish</h6>
                                </div>
                                <!-- Start: card body -->
                                <div class="add-product__body px-sm-40 px-20">
                                    <!-- Start: form -->
                                    <form method="post" action="" >
                                        <div class="form-group">
                                            <label for="Depots" style="text-transform: none;"><b class="text-danger">*</b>Omborlar </label>
                                            <select name="DepotID" class=" form-control" id="Depots">
                                                <option value="0">Mahsulot joylashgan omborni tanlang</option>
                                                <% for (let i = 0; i < depot.length; i++) { %>
                                                    <option value="<%= depot[i].DepotID %>"><%= depot[i].DepotName %></option>
                                                <% }%>
                                            </select>
                                        </div>
                                        <!-- form group 2 -->
                                        <div class="form-group ">
                                            <div class="countryOption">
                                                <label for="countryOption">
                                                    <b class="text-danger">*</b>Kategoriya
                                                </label>
                                                <select class="js-example-basic-single js-states form-control" id="countryOption">
                                                    <option value="0">Mahsulot kategoriyasini tanlang</option>
                                                    <% for (const category of categories) { %>
                                                        <option value="<%= category.CategoryID %>"><%= category.CategoryName %></option>
                                                    <%} %>
                                                </select>
                                            </div>
                                        </div>

                                        <!-- form group -->
                                        <div class="form-group">
                                            <label for="name1" style="text-transform: none;"><b class="text-danger">*</b>Mahsulot nomi</label>
                                            <input type="text" class="form-control" id="name1" placeholder="">
                                        </div>
                                        <!-- form group -->
                                        <div class="form-group">
                                            <label for="namet" style="text-transform: none;"><b class="text-danger">*</b>Mahsulotning qisqacha tavsifi</label>
                                            <input name="ShortDescription" type="text" class="form-control" id="namet" placeholder="">
                                        </div>
                                        <div class="form-group  mb-0">
                                            <label for="asduz" style="text-transform: none;"> <b class="text-danger">*</b>Mahsulot haqida</label>
                                            <textarea name="Description" class="form-control" id="asduz" rows="3" placeholder=""></textarea>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-lg-6 col-sm-12">
                                                <label for="Miqdorii" style="text-transform: none;"><b class="text-danger">*</b>Miqdori</label>
                                                <input name="Quantity" type="text" class="form-control" id="Miqdorii" placeholder="">
                                            </div>

                                            <div class="form-group col-lg-6 col-sm-12">
                                                <label for="name2">O'lchov birligi</label>
                                                <div class=" select-style2">
                                                    <div class="atbd-select ">
                                                        <select name="select-alerts2" id="select-alerts2" class="form-control ">
                                                            <option value="kg">O'lchov birligini tanlang</option>
                                                            <option value="kg">Kg</option>
                                                            <option value="piece">Dona</option>
                                                            <option value="block">Blok</option>
                                                            <option value="liter">Litr</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group col-lg-6 col-sm-12">
                                                <label for="name2">Brend</label>
                                                <div class=" select-style2">
                                                    <div class="atbd-select ">
                                                        <select name="select-alerts2" id="select-alerts2" class="form-control ">
                                                            <option value="ID">Brandni tanlang</option>
                                                            <option value="ID">Coca cola</option>
                                                            <option value="ID">Zara</option>
                                                            <option value="ID">Gucci</option>
                                                            <option value="ID">Apple</option>
                                                            <option value="ID">Samsung</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <input class="checkbox" type="checkbox" id="brend">
                                                <label for="brend">
                                                    <span class="checkbox-text">
                                                                           Mavjud emas
                                                    </span>
                                                </label>
                                            </div>
                                            <div class="form-group col-lg-6 col-sm-12">
                                                <label for="Barcode" style="text-transform: none;">Barcode</label>
                                                <input type="text" class="form-control" id="Barcode" placeholder="">
                                            </div>

                                            <div class="form-group col-lg-6 col-sm-12">
